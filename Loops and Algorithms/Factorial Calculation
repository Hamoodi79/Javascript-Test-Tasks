// Write a function named calculateFactorial that takes an integer n as input and returns the factorial of n. Assume n is a non-negative integer.

// Factorial Calculation 
function calculateFactorial(n) {
     if (n === 0 || n === 1) {
        return 1;
    } else {
        let factorial = 1;
        for (let i = 2; i <= n; i++) {
            factorial *= i;
        }
        return factorial;
    }
}

